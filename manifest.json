{
  "manifest_version": 3,
  "name": "سَیدَگنج",
  "version": "1.0",
  "description": "سَیدَگنج ءِ تہا گالانی شوہازگ ءِ ھاترا اے ایکسٹینشن ءَ کارمرز کن اِت",
  "permissions": ["contextMenus", "activeTab", "storage", "sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
